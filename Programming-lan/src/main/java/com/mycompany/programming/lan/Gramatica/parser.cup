info::= NAME TWOPOINT ID:e 
{:
RESULT=getInfos(1,e);
:}
;

info::= VER TWOPOINT ver:e 
{:
RESULT=getInfos(2,e);
:};

info::= AUTHOR  TWOPOINT ID:e 
{:
RESULT=getInfos(3,e);
:};

info::= EXTENS  TWOPOINT ID:e 
{:
RESULT=getInfos(4,e);
:}
| EXT_ID:e 
{:
String vector[]=e.split(":");
switch(vector[0]){
   case "autor":
   RESULT=getInfos(3,vector[1]);
   break;
   case "extension":
   RESULT=getInfos(4,vector[1]);
   break;
   case "nombre":
   RESULT=getInfos(1,vector[1]);
   break;
   default:
     this.parser.addError(1,eleft,eright,e,"Token invalido, no forma parte de la informacion del lenguaje");
   break;
   
}
:};

