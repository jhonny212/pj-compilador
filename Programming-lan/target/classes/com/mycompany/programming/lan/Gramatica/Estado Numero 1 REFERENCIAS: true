Estado Numero 1 REFERENCIAS: true
Estado Numero 0 REFERENCIAS: false
    CON W IR A   1
    CON WB IR A   1




 void analizar() {
        for (int i = 0; i < this.listado.size(); i++) {
            generarAFD afd = this.listado.get(i);
            previo=0;
            index=-1;
            estado es = afd.AFD.get(afd.estadoInicial);
            value = "";
            String string="";
            for (int j=0;j<es.referencias.size();j++) {
                referencia ref =es.referencias.get(j);
                if (txt.startsWith(ref.val)) {
                    String tmp = txt.replaceFirst(ref.val, "");
                    if(ref.val.length()>previo){
                        previo=ref.val.length();
                        index=j;
                        value=ref.val;
                        string=tmp;
                    }
                }
            }
            if(index!=-1){
               this.txt=string;
            
            }
        }



 for (int i = 0; i < this.listado.size(); i++) {
            generarAFD afd = this.listado.get(i);
            estado es = afd.AFD.get(afd.estadoInicial);
            for (referencia ref : es.referencias) {
                value = "";
                if (txt.startsWith(ref.val)) {
                    String tmp = txt.replaceFirst(ref.val, "");
                    value += ref.val;
                    txt = tmp;
                    analizar(ref.y);
                    if (this.aceptacion) {
                        System.out.println("se acepto " + ref.y.nombreToken + " valor " + value);
                        i = -1;
                        this.tmp = txt;
                        this.aceptacion = false;
                    } else {
                        this.txt = this.tmp;
                    }
                }
            }
        }